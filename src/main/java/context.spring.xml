<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

<bean id="ar" class="com.wolken.wolkenapp.bean.AirportBean">
<property name="airport_name" value="bangalore airport"></property>
<property name="destination" value="India"></property>
<property name="flight_ticket_no" value="132"></property>
<property name="ticket_price" value="5000"></property>

</bean>

</beans>